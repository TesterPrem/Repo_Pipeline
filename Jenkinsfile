pipeline
{
agent any
stages
  {
   stage("Build")
   {
    steps
    { 
      echo "Building Project"
    }
   }
   stage("Deploy")
   {
    steps
    {
      echo "Deploying the project"
    }
   }
   stage("Test")
   {
    steps
    {
      echo "Testing the Project"
    }
   }
   stage("Release")
   {
    steps
    {
      echo "Releasing the Project"
    }
   }
}
     
